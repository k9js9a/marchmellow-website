    "postinstall": "prisma generate",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "commitlint": "commitlint --config commitlint.config.ts --verbose --edit",
    "prepare": "husky"
